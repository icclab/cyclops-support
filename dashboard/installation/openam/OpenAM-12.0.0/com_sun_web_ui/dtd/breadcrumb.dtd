<!-- This is the descriptor for a breadcrumb. -->
<!ELEMENT breadcrumb (link*, page)>

<!-- This is the XML description for a breadcrumb link -->
<!ELEMENT link (#PCDATA)>
<!ATTLIST link
	href        CDATA #REQUIRED
	mouseOver   CDATA #REQUIRED
	onClick	    CDATA #IMPLIED
	target      CDATA #IMPLIED	
	hrefValue   CDATA #IMPLIED
>

<!-- This is the XML description for the breadcrumb current page -->
<!ELEMENT page (#PCDATA)>
